<?xml version="1.0" encoding="UTF-8" ?>
<openbox_menu>
  <menu id="root-menu" label="OpenBox 3">
    <menu id="1" label="Editors">
      <item label="Vim">
        <action name="Execute">
          <execute>alacritty -e vim</execute>
        </action>
      </item>
      <item label="VSCodium">
        <action name="Execute">
          <execute>codium</execute>
        </action>
      </item>
    </menu>
    <menu id="2" label="DevKit">
      <item label="AndroidStudio">
        <action name="Execute">
          <execute>android_studio</execute>
        </action>
      </item>
    </menu>
    <menu id="3" label="Multimedia">
      <item label="ALSA mixer">
        <action name="Execute">
          <execute>alacritty -e alsamixer</execute>
        </action>
      </item>
      <item label="PulseAudio">
        <action name="Execute">
          <execute>pavucontrol</execute>
        </action>
      </item>
    </menu>
    <menu id="4" label="Shells">
      <item label="Alacritty">
        <action name="Execute">
          <execute>alacritty</execute>
        </action>
      </item>
      <item label="Files">
        <action name="Execute">
          <execute>pcmanfm</execute>
        </action>
      </item>
    </menu>
    <menu id="5" label="System">
      <item label="Network Connections">
        <action name="Execute">
          <execute>alacritty -e nmtui</execute>
        </action>
      </item>
      <item label="Htop">
        <action name="Execute">
          <execute>alacritty -e htop</execute>
        </action>
      </item>
    </menu>
    <separator />
    <menu id="6" label="OpenBox">
      <menu id="client-list-menu" />
      <item label="Reconfigure">
        <action name="Reconfigure" />
      </item>
      <separator />
      <item label="Exit">
        <action name="Exit" />
      </item>
    </menu>
  </menu>
</openbox_menu>
